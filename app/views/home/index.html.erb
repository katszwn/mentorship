<h1>Our Mentors:</h1>

<% @users.each do |u| =%>
  <p>
    <%= u.first_name + " " + u.last_name %>
    <br>
    Expertise area:
    <%= u.area_of_expertise %>
  </p>
<% end %>
<% if user_signed_in? %>
  <%= link_to "Log Out", destroy_user_session_path, :method => :delete %>
<% else %>
  <%= link_to('Login', new_user_session_path) %>
<% end %>
